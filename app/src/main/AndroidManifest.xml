<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sm.ej.nk.homeal">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE"/>

    <permission android:name="com.sm.ej.nk.homeal.permission.C2D_MESSAGE"
        android:protectionLevel="signature"/>
    <uses-permission android:name="com.sm.ej.nk.homeal.permission.C2D_MESSAGE"/>


    <application
        android:name=".HomealApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".CkMainActivity"
            android:exported="true" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".MapActivity" />
        <activity android:name=".ChattingActivity" />
        <activity android:name=".CkWriteReViewActivity" />
        <activity android:name=".CkPersonalDataActivity"
            android:exported="true"/>
        <activity android:name=".SettingActivity"
            android:exported="true"/>
        <activity android:name=".AdviceActivity"
            android:exported="true"/>
        <activity android:name=".MenuAddActivity" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".EtWriteReviewActivity"
            android:exported="true"/>
        <activity android:name=".EtPersonalDataActivity"
            android:exported="true"/>
        <activity android:name=".EtZzimActivity" />
        <activity android:name=".InfoCkDetailActivity" />
        <activity android:name=".MenuDetailActivity" />
        <activity android:name=".ReserveRequestActivity" />
        <activity android:name=".EtMainActivity"
            android:exported="true"></activity>

        <receiver
            android:name="com.google.android.gms.gcm.GcmReceiver"
            android:exported="true"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <category android:name="com.example.gcm" />
            </intent-filter>
        </receiver>

        <service
            android:name=".gcm.MyGcmListenerService"
            android:exported="false" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
            </intent-filter>
        </service>
        <service
            android:name=".gcm.MyInstanceIDListenerService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.android.gms.iid.InstanceID" />
            </intent-filter>
        </service>

        <service
            android:name=".gcm.RegistrationIntentService"
            android:exported="false">
        </service>
    </application>

</manifest>